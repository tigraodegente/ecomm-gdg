    "marked": "^15.0.6",
    "nodemailer": "^6.10.0",
    "ora": "^8.1.1",
    "trix": "^2.1.12"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/alpinejs": "^3.13.11",
    "@types/better-sqlite3": "^7.6.12",
    "daisyui": "^4.12.23",
    "netlify-cli": "^18.0.2",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
