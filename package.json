    "@astrojs/alpinejs": "^0.4.3",
    "@astrojs/check": "^0.9.4",
    "@astrojs/db": "^0.14.6",
    "@astrojs/netlify": "^6.1.0",
    "@astrojs/tailwind": "^5.1.5",
    "@iconify-json/lucide": "^1.2.24",
    "@iconify-json/lucide-lab": "^1.2.3",
    "@types/nodemailer": "^6.4.17",
    "alpinejs": "^3.14.8",
    "astro": "^5.1.9",
    "astro-iconify": "^1.2.0",
    "astro-seo": "^0.8.4",
    "better-auth": "^1.1.14",
    "better-sqlite3": "^11.8.1",
